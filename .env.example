# STS Neural Agent - Environment Variables
# Copy this file to .env and update with your values

# Weights & Biases Configuration
WANDB_API_KEY=your_wandb_api_key_here
WANDB_PROJECT=sts-neural-agent
WANDB_ENTITY=your_username_or_team

# CUDA Configuration (if using GPU)
CUDA_VISIBLE_DEVICES=0

# Training Defaults
STS_EPISODES=2000
STS_REWARD_FUNCTION=comprehensive
STS_BATCH_SIZE=64
STS_LEARNING_RATE=3e-4

# Docker Configuration
COMPOSE_PROJECT_NAME=sts-neural-agent

# Performance Tuning
PYTHONUNBUFFERED=1
OMP_NUM_THREADS=4